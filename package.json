{
  "name": "email-queue-service",
  "version": "1.0.0",
  "description": "AWS-based email queue service for processing bulk emails",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "deploy:lambda": "cd lambda && zip -r ../email-processor.zip . && aws lambda update-function-code --function-name email-processor --zip-file fileb://../email-processor.zip"
  },
  "dependencies": {
    "express": "^4.18.2",
    "aws-sdk": "^2.1490.0",
    "joi": "^17.11.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.53.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1"
  },
  "keywords": [
    "email",
    "queue",
    "aws",
    "sqs",
    "lambda",
    "nodejs"
  ],
  "author": "",
  "license": "MIT"
}